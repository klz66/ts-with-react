tent.match(/((?<=<h6>).+?)(?=<\\/h6>)/))) {\n      return content.match(/((?<=<h6>).+?)(?=<\\/h6>)/)[0].replace(re1,'');\n    }\n    else{\n      return ''\n    } \n  }\n  \n\t\t\treturn (\n        <div style={{display:'flex',height:'100vh',paddingLeft:'100px'}}>\n          <div style={{width:'180px',marginRight:'50px'}}>\n            <div className='goHome' onClick={()=>{ props.history.push('/')}}>\n              回首页\n            </div>\n            <List\n              size=\"small\"\n              style = {{overflowY:'scroll'}}\n              header={<div onClick={()=>handAdd()} style={{cursor:'pointer'}}>新建文章</div>}\n              footer={\n                <Popconfirm\n                title=\"确定清除草稿箱？\"\n                onConfirm={()=>confirmDeleteAll()}\n                okText=\"Yes\"\n                cancelText=\"No\"\n              ><div>             \n              清除草稿箱\n              </div>\n              </Popconfirm>\n              }\n              bordered\n              dataSource={draftList}\n              renderItem={(item,index) => <List.Item style={{display:'flex',justifyContent:'space-between',backgroundColor: index === actice?'#ddd':''}} onClick={()=>{\n                changeContent(index,item);\n              }}>{item.title}\n              <div className='icon-margin'>\n              <Popconfirm\n                title=\"确认直接发表\"\n                onConfirm={()=>handPost(item.content)}\n                okText=\"Yes\"\n                cancelText=\"No\"\n              >\n              <CheckCircleOutlined  />\n              </Popconfirm>\n              <Popconfirm\n                title=\"确认删除此草稿\"\n                onConfirm={()=>confirmDelete(item.id)}\n                okText=\"Yes\"\n                cancelText=\"No\"\n              >\n                <StopOutlined />\n              </Popconfirm>\n              </div>\n              </List.Item>}\n            />\n          </div>\n\t\t\t\t<div>\n          <Editor\n            ref={editorRef}\n     