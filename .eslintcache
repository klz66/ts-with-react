me}</div>\n                <div style={{fontSize:'12px',color:'#969696'}}>于{item.gmtCreate.slice(0,10)}加入&nbsp;&nbsp;\n                  发表了{item.blogNum}篇\n                </div>\n              </div>\n              <div style={{position:'absolute',right:'0px'}}>\n                { !item.focus && <span style={{color:'green'}} onClick={()=>{handleFocus(item.id)}}><PlusOutlined />关注</span>}\n                { item.focus && <span style={{color:'#999999'}} onClick={()=>{handleCancelFocus(item.id)}}><CheckOutlined />已互关{item.focus}</span>}\n              </div>\n            </div>\n            \n          </List.Item>\n        )}\n      />\n    </div>\n  );\n}\n\nexport default withRouter(FansList);\n","C:\\Users\\13788\\Desktop\\kl\\github\\ts-with-react\\src\\pages\\write\\edit.js",["282","283","284","285"],{"ruleId":"286","severity":1,"message":"287","line":24,"column":5,"nodeType":"288","endLine":24,"endColumn":7,"suggestions":"289"},{"ruleId":"290","replacedBy":"291"},{"ruleId":"292","replacedBy":"293"},{"ruleId":"286","severity":1,"message":"294","line":21,"column":5,"nodeType":"288","endLine":21,"endColumn":7,"suggestions":"295"},{"ruleId":"296","severity":1,"message":"297","line":62,"column":24,"nodeType":"298","messageId":"299","endLine":62,"endColumn":25,"suggestions":"300"},{"ruleId":"296","severity":1,"message":"301","line":62,"column":26,"nodeType":"298","messageId":"299","endLine":62,"endColumn":27,"suggestions":"302"},{"ruleId":"296","severity":1,"message":"297","line":62,"column":33,"nodeType":"298","messageId":"299","endLine":62,"endColumn":34,"suggestions":"303"},{"ruleId":"296","severity":1,"message":"301","line":62,"column":35,"nodeType":"298","messageId":"299","endLine":62,"endColumn":36,"suggestions":"304"},{"ruleId":"296","severity":1,"message":"297","line":62,"column":41,"nodeType":"298","messageId":"299","endLine":62,"endColumn":42,"suggestions":"305"},{"ruleId":"296","severity":1,"message":"301","line":62,"column":43,"nodeType":"298","messageId